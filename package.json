{
  "name": "buildkit-stylus",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "description": "BuildKit (Stylus Edition) - Developer infra playground for Arbitrum",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "stylus:build": "pnpm --filter stylus-contracts build",
    "stylus:deploy": "pnpm --filter stylus-contracts deploy",
    "indexer:start": "pnpm --filter indexer start",
    "dashboard:dev": "pnpm --filter dashboard dev"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bufferutil",
      "esbuild",
      "keccak",
      "unrs-resolver",
      "utf-8-validate"
    ]
  }
}
